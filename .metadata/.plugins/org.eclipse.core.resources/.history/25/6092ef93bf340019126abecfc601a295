package com.splitwword;

import java.awt.List;
import java.util.ArrayList;

public class SplitoddWord{
	static ArrayList <String>str=new ArrayList<>();
public static void main(String[] args) {
	str.add("mirza");
	str.add("beg");
	str.add("prashant");
	str.add("people");
	
}
public static <T> void remove(str<T> list, Predicate<T> predicate)
{
	list.removeIf(x -> predicate.test(x));
}
}
